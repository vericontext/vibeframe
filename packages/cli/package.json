{
  "name": "@vibeframe/cli",
  "version": "0.20.0",
  "description": "VibeFrame CLI - natural language video editing from the terminal",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vericontext/vibeframe.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/vericontext/vibeframe/issues"
  },
  "keywords": [
    "vibeframe",
    "cli",
    "video-editing",
    "ai",
    "ffmpeg",
    "commander"
  ],
  "type": "module",
  "bin": {
    "vibe": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./engine": {
      "import": "./src/engine/index.ts",
      "types": "./src/engine/index.ts"
    },
    "./commands/export": {
      "import": "./src/commands/export.ts",
      "types": "./src/commands/export.ts"
    },
    "./commands/ai-edit": {
      "import": "./src/commands/ai-edit.ts",
      "types": "./src/commands/ai-edit.ts"
    },
    "./commands/ai-analyze": {
      "import": "./src/commands/ai-analyze.ts",
      "types": "./src/commands/ai-analyze.ts"
    },
    "./commands/ai-review": {
      "import": "./src/commands/ai-review.ts",
      "types": "./src/commands/ai-review.ts"
    },
    "./commands/ai-image": {
      "import": "./src/commands/ai-image.ts",
      "types": "./src/commands/ai-image.ts"
    },
    "./commands/ai-script-pipeline": {
      "import": "./src/commands/ai-script-pipeline.ts",
      "types": "./src/commands/ai-script-pipeline.ts"
    },
    "./commands/ai-highlights": {
      "import": "./src/commands/ai-highlights.ts",
      "types": "./src/commands/ai-highlights.ts"
    },
    "./commands/ai-narrate": {
      "import": "./src/commands/ai-narrate.ts",
      "types": "./src/commands/ai-narrate.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "tsx src/index.ts",
    "lint": "eslint --ext .ts,.tsx src/",
    "test": "vitest"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@google/generative-ai": "^0.21.0",
    "@vibeframe/ai-providers": "workspace:*",
    "@vibeframe/core": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^17.2.3",
    "music-metadata": "^11.11.1",
    "openai": "^4.77.0",
    "ora": "^8.0.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "1",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=18"
  }
}
