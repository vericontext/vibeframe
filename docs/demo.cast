[0.001, "o", "\u001b[0m\u001b[2m  AI-native video editing in the terminal\u001b[0m\r\n\u001b[2m  Mode: dry-run\u001b[0m\r\n\r\n"]
[2.009, "o", "\r\n\u001b[44m\u001b[1;37m                                                                \r\n"]
[0.000, "o", "              ACT 1: One Command Video Production               \r\n                                                                "]
[0.000, "o", "\u001b[0m\r\n\r\n\u001b[2m# One script in, full video out — a single command produces a finished video\u001b[0m\r\n\r\n"]
[1.512, "o", "\u001b[2m# AI auto-generates: storyboard → narration → images → video → project file\u001b[0m\r\n\r\n"]
[0.000, "o", "\u001b[0;32m$ vibe ai script-to-video \\\u001b[0m\r\n"]
[0.000, "o", "\u001b[0;32m    \"A 30-second product launch video for an AI video editor. Sce...\" \\\u001b[0m\r\n\u001b[0;32m    --voice rachel \\\u001b[0m\r\n\u001b[0;32m    --image-provider gemini \\\u001b[0m\r\n\u001b[0;32m    --generator kling\u001b[0m\r\n\r\n\u001b[2m# Running pipeline...\u001b[0m\r\n\r\n\u001b[2m  [1/5] Generating storyboard with Claude...\u001b[0m\r\n"]
[0.811, "o", "\u001b[2m        3 scenes, 30s total duration\u001b[0m\r\n"]
[0.313, "o", "\u001b[2m  [2/5] Generating narration with ElevenLabs (voice: rachel)...\u001b[0m\r\n"]
[0.813, "o", "\u001b[2m        scene-1.mp3 (10.2s) scene-2.mp3 (9.8s) scene-3.mp3 (10.0s)\u001b[0m\r\n"]
[0.308, "o", "\u001b[2m  [3/5] Generating images with Gemini...\u001b[0m\r\n"]
[0.814, "o", "\u001b[2m        scene-1.png scene-2.png scene-3.png\u001b[0m\r\n"]
[0.310, "o", "\u001b[2m  [4/5] Generating videos with Kling v2.5...\u001b[0m\r\n"]
[0.813, "o", "\u001b[2m        scene-1.mp4 (10s) scene-2.mp4 (10s) scene-3.mp4 (10s)\u001b[0m\r\n"]
[0.313, "o", "\u001b[2m  [5/5] Assembling project...\u001b[0m\r\n"]
[0.512, "o", "\r\n\u001b[0;32m  Done! Project: my-project.vibe.json\u001b[0m\r\n"]
[0.000, "o", "\u001b[0;32m  Output:  script-to-video-output/final.mp4 (30s, 1080p)\u001b[0m\r\n\r\n\u001b[2m# One text in, finished video out. Storyboard, narration, images, video — all AI-generated.\u001b[0m\r\n"]
[2.011, "o", "\r\n\u001b[44m\u001b[1;37m                                                                "]
[0.000, "o", "\r\n"]
[0.000, "o", "                  ACT 2: Post-Production Combo                  \r\n                                                                \u001b[0m\r\n\r\n"]
[0.000, "o", "\u001b[2m# 4-hit post-production combo — denoise → silence cut → captions → fade\u001b[0m\r\n\r\n"]
[1.510, "o", "\u001b[2m# Step 1: Remove audio noise\u001b[0m\r\n"]
[0.000, "o", "\u001b[0;32m$ vibe ai noise-reduce demo-output/sample.mp4 -o demo-output/step1-clean.mp4\u001b[0m\r\n\u001b[2m  (dry-run: skipped)\u001b[0m\r\n\r\n"]
[0.000, "o", "\u001b[2m  Noise profile: -30dB detected\u001b[0m\r\n"]
[0.000, "o", "\u001b[2m  Applied: highpass=200, lowpass=3000, afftdn=nr=20\u001b[0m\r\n"]
[0.000, "o", "\u001b[0;32m  Cleaned: step1-clean.mp4\u001b[0m\r\n"]
[0.000, "o", "\r\n"]
[0.513, "o", "\u001b[2m# Step 2: Auto-cut silent segments\u001b[0m\r\n"]
[0.000, "o", "\u001b[0;32m$ vibe ai silence-cut demo-output/step1-clean.mp4 -o demo-output/step2-cut.mp4 --noise -35 --min-duration 0.5\u001b[0m\r\n\u001b[2m  (dry-run: skipped)\u001b[0m\r\n"]
[0.000, "o", "\r\n"]
[0.000, "o", "\u001b[2m  Detected 12 silent segments (total: 18.4s)\u001b[0m\r\n"]
[0.000, "o", "\u001b[2m  Removed 12 segments, saved 18.4s\u001b[0m\r\n"]
[0.000, "o", "\u001b[2m  Duration: 62.0s -> 43.6s (-29.7%)\u001b[0m\r\n"]
[0.000, "o", "\u001b[0;32m  Trimmed: step2-cut.mp4\u001b[0m\r\n"]
[0.000, "o", "\r\n"]
[0.508, "o", "\u001b[2m# Step 3: Transcribe + burn captions\u001b[0m\r\n\u001b[0;32m$ vibe ai caption demo-output/step2-cut.mp4 -o demo-output/step3-captioned.mp4 --style bold\u001b[0m\r\n"]
[0.000, "o", "\u001b[2m  (dry-run: skipped)\u001b[0m\r\n"]
[0.000, "o", "\r\n"]
[0.000, "o", "\u001b[2m  Transcribing with Whisper...\u001b[0m\r\n\u001b[2m  Found 34 segments (43.6s)\u001b[0m\r\n"]
[0.000, "o", "\u001b[2m  Burning captions with style: bold (white, black outline)\u001b[0m\r\n"]
[0.000, "o", "\u001b[0;32m  Captioned: step3-captioned.mp4\u001b[0m\r\n\r\n"]
[0.512, "o", "\u001b[2m# Step 4: Add fade in/out effects\u001b[0m\r\n"]
[0.000, "o", "\u001b[0;32m$ vibe ai fade demo-output/step3-captioned.mp4 -o demo-output/step4-final.mp4 --fade-in 1.0 --fade-out 1.5\u001b[0m\r\n"]
[0.001, "o", "\u001b[2m  (dry-run: skipped)\u001b[0m\r\n\r\n"]
[0.000, "o", "\u001b[2m  Applied: fade-in 1.0s (video+audio), fade-out 1.5s (video+audio)\u001b[0m\r\n\u001b[0;32m  Final: step4-final.mp4\u001b[0m\r\n"]
[0.000, "o", "\r\n\r\n"]
[0.000, "o", "\u001b[2m# 4 commands, done: denoise → silence cut → captions → fade\u001b[0m\r\n"]
[0.000, "o", "\u001b[2m# 62s raw → 43.6s polished (zero manual editing)\u001b[0m\r\n"]
[2.009, "o", "\r\n\u001b[44m\u001b[1;37m                                                                \r\n"]
[0.000, "o", "                 ACT 3: Let the Agent Handle It                 \r\n                                                                \u001b[0m\r\n\r\n"]
[0.000, "o", "\u001b[2m# Let the agent handle it — give complex tasks in plain English\u001b[0m\r\n"]
[0.000, "o", "\r\n"]
[1.508, "o", "\u001b[0;32m$ vibe agent -i \"Analyze demo-output/sample.mp4, find the best frame for a thumbnail, extract it, and generate captions for the video.\" -v\u001b[0m\r\n\r\n"]
[0.000, "o", "\u001b[2m# Agent executing autonomously...\u001b[0m\r\n"]
[0.001, "o", "\r\n\u001b[0;35m  [Agent] Planning: 3 tasks identified\u001b[0m\r\n"]
[0.608, "o", "\u001b[0;35m  [Agent] Step 1: Calling ai_analyze...\u001b[0m\r\n"]
[0.810, "o", "\u001b[2m          Video: 43.6s, 1080p, 30fps\u001b[0m\r\n"]
[0.000, "o", "\u001b[2m          Content: tech product demo, speaker with screen recording\u001b[0m\r\n"]
[0.000, "o", "\u001b[2m          Audio: clear speech, minimal background noise\u001b[0m\r\n"]
[0.511, "o", "\u001b[0;35m  [Agent] Step 2: Calling ai_thumbnail --best-frame...\u001b[0m\r\n"]
[0.814, "o", "\u001b[2m          Analyzed 15 candidate frames\u001b[0m\r\n\u001b[2m          Best frame: 00:00:12.4 (score: 0.94)\u001b[0m\r\n"]
[0.000, "o", "\u001b[2m          Saved: demo-output/thumbnail.png (1920x1080)\u001b[0m\r\n"]
[0.512, "o", "\u001b[0;35m  [Agent] Step 3: Calling ai_caption...\u001b[0m\r\n"]
[0.809, "o", "\u001b[2m          Transcribed 34 segments\u001b[0m\r\n\u001b[2m          Burned captions with style: bold\u001b[0m\r\n"]
[0.000, "o", "\u001b[2m          Output: demo-output/captioned.mp4\u001b[0m\r\n"]
[0.512, "o", "\r\n"]
[0.000, "o", "\u001b[0;35m  [Agent] Done! Completed 3 tool calls autonomously.\u001b[0m\r\n\r\n"]
[0.000, "o", "\u001b[2m# One sentence in → Agent analyzes, extracts thumbnail, generates captions autonomously\u001b[0m\r\n"]
[2.009, "o", "\r\n\u001b[44m\u001b[1;37m"]
[0.000, "o", "                                                                \r\n"]
[0.000, "o", "                ACT 4: Motion Graphics Pipeline                 \r\n                                                                "]
[0.000, "o", "\u001b[0m\r\n"]
[0.000, "o", "\r\n"]
[0.000, "o", "\u001b[2m# Natural language to motion graphics — Claude writes Remotion TSX and renders it\u001b[0m\r\n\r\n"]
[1.511, "o", "\u001b[2m# Step 1: Generate a title card from plain English\u001b[0m\r\n\r\n"]
[0.000, "o", "\u001b[0;32m$ vibe ai motion \\\u001b[0m\r\n\u001b[0;32m    \"cinematic title card with 'VIBEFRAME' text, spring bounce from zero to full size, gold gradient color, particle effects in background\" \\\u001b[0m\r\n"]
[0.000, "o", "\u001b[0;32m    --render -o demo-output/title.webm\u001b[0m\r\n"]
[0.000, "o", "\r\n\u001b[2m# Generating motion graphic...\u001b[0m\r\n\r\n\u001b[2m  [Claude] Generating Remotion TSX component...\u001b[0m\r\n"]
[0.813, "o", "\u001b[2m          import { spring, useCurrentFrame } from 'remotion';\u001b[0m\r\n\u001b[2m          // 47 lines of React motion graphics code\u001b[0m\r\n"]
[0.514, "o", "\u001b[2m  [Render] Scaffolding temp project...\u001b[0m\r\n"]
[0.610, "o", "\u001b[2m  [Render] npx remotion render → title.webm (1920x1080, 5s, 30fps)\u001b[0m\r\n"]
[0.807, "o", "\r\n"]
[0.000, "o", "\u001b[0;32m  Motion graphic rendered: demo-output/title.webm\u001b[0m\r\n\r\n"]
[1.515, "o", "\u001b[2m# Step 2: Composite a lower-third overlay onto video\u001b[0m\r\n\r\n"]
[0.000, "o", "\u001b[0;32m$ vibe ai motion \\\u001b[0m\r\n"]
[0.000, "o", "\u001b[0;32m    \"lower-third title: 'Kiyeon, CEO' with smooth slide-in from left, semi-transparent dark background bar\" \\\u001b[0m\r\n\u001b[0;32m    --video demo-output/sample.mp4 -o demo-output/with-title.mp4\u001b[0m\r\n\r\n\u001b[2m# Compositing overlay...\u001b[0m\r\n\r\n\u001b[2m  [Claude] Generating Remotion TSX component...\u001b[0m\r\n"]
[0.811, "o", "\u001b[2m          // Lower-third with slide-in animation\u001b[0m\r\n"]
[0.512, "o", "\u001b[2m  [Render] Rendering transparent overlay (1920x1080, 3s)...\u001b[0m\r\n"]
[0.610, "o", "\u001b[2m  [Composite] Overlaying on sample.mp4 via FFmpeg...\u001b[0m\r\n"]
[0.814, "o", "\u001b[2m          ffmpeg -i sample.mp4 -i overlay.webm -filter_complex overlay\u001b[0m\r\n"]
[0.512, "o", "\r\n"]
[0.000, "o", "\u001b[0;32m  Composited: demo-output/with-title.mp4\u001b[0m\r\n\r\n"]
[0.000, "o", "\u001b[2m# Plain English → Claude generates code → Remotion renders → FFmpeg composites\u001b[0m\r\n"]
[0.000, "o", "\u001b[2m# Motion graphics in the terminal — no After Effects needed\u001b[0m\r\n"]
[2.011, "o", "\r\n\r\n"]
